<%= form_for @post, :url => { :action => "create" }, :html => { :multipart => true } do |f| %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description, :rows => 10 %>
  </p>
  <p>
    <%= f.file_field :photo %>
  </p>
  <p>
    <%= check_box_tag 'whiskey', 'selected' %> Whiskey <br /> 
    <%= check_box_tag 'gin', 'selected' %> Gin
  </p>
  <%= hidden_field_tag(:posted_by, current_user.id) %>
  <p><%= f.submit "Submit" %></p>
  
  <% @venues.10 do |v| %>
    <%= v.name %> <br />
  <% end %>
<% end %>